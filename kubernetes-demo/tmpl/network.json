{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "variables": {},
  "resources": [{
    "type": "Microsoft.Network/virtualNetworks",
    "name": "[parameters('virtualNetworkName')]",
    "apiVersion": "2016-03-30",
    "comments": "Virtual network for overall solution",
    "tags": {
      "displayName": "Virtual Network"
    },
    "dependsOn": [],
    "properties": {
      "addressSpace": {
        "addressPrefixes": [
          "10.0.0.0/16"
        ]
      },
      "subnets": [{
        "name": "[parameters('subnet1Name')]",
        "properties": {
          "addressPrefix": "10.0.0.0/20 "
        }},
        {
          "name": "[parameters('subnet2Name')]",
          "properties": {
            "addressPrefix": "10.0.16.0/20 "
          }
      }]
    }
  }],
  "outputs": {
    "virtualNetworkName": {
      "type": "string",
      "value": "[parameters('virtualNetworkName')]"
    },
    "virtualNetworkResourceGroup": {
      "type": "string",
      "value": "[resourceGroup().name]"
    },
    "subnet1Name": {
      "type": "string",
      "value": "[parameters('subnet1Name')]"
    },
    "subnet1ID": {
      "type": "string",
      "value": "[resourceId(resourceGroup().name,'Microsoft.Network/virtualNetworks/subnets', parameters('virtualNetworkName'), parameters('subnet1Name'))]"
    }
  }
}